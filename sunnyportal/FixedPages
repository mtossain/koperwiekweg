<script type="text/javascript">
    function SetConfirmTouSaveVisible(checkbox)
    {
        var buttonId = checkbox.id.replace("ConfirmTouCheckbox", "ConfirmTouSaveButton");
        if ($('#' + buttonId)) {
            if (checkbox.checked) {
                $('#' + buttonId).show();
                $('#' + buttonId + "Disabled").hide();
            }
            else {
                $('#' + buttonId).hide();
                $('#' + buttonId + "Disabled").show();
            }
        }
    }

    function OpenPrintVersion(touDocKey) {
        window.open("../Templates/PrintText.aspx?content=touDocKey_" + touDocKey, "PrintWindow", "width=800,height=600,left=100,top=100,scrollbars=yes");
    }
    
</script>

