<script type="text/javascript">
    var __lastSuccess = null;
    $(document).ready(function () {
        var cbSuccess = function (json, textStatus, jqXHR) {
            if (json.Info["currentPlantPower"] != null) {
                $("#CurrentPlantPowerValue").html(json.Info["currentPlantPower"]);
                if (json.Info["currentPlantPowerUnit"] != null) {
                    $("#CurrentPlantPowerUnit").html(json.Info["currentPlantPowerUnit"]);
                }
                $("#CurrentPlantPowerTimestamp").html("&nbsp;");
                if (json.Info["currentPlantTime"] != null) {
                    $("#CurrentPlantPowerTimestamp").attr("title", json.Info["currentPlantTime"]);
                }
                __lastSuccess = new Date().getTime();
                if (json.Info["currentPlantPowerPointerAngle"] != null) {
                    adjustGauge(json.Info["currentPlantPowerPointerAngle"]);
                }
            }
        }
        var cbError = function (jqXHR, errorType, exception) {
            var elem = $("#CurrentPlantPowerTimestamp");
            if (elem != null && __lastSuccess != null) {
                try {
                    var deltaSec = (new Date().getTime() - __lastSuccess) / 1000;
                    if (deltaSec <= 15) {
                        elem.html("&nbsp;");
                    } else if (deltaSec <= 60) {
                        elem.html(jsUtil.getData("some_seconds_ago", "PlantPowerWidgetControl"));
                    } else if (deltaSec <= 900) {
                        elem.html(jsUtil.getData("just_now", "PlantPowerWidgetControl"));
                    } else if (deltaSec < 3600) {
                        elem.html(jsUtil.getData("x_minutes_ago", "PlantPowerWidgetControl").replace("{0}", Math.floor(deltaSec / 60)));
                    } else if (deltaSec < 7200) {
                        elem.html(jsUtil.getData("one_hours_ago", "PlantPowerWidgetControl"));
                    } else {
                        elem.html(jsUtil.getData("x_hours_ago", "PlantPowerWidgetControl").replace("{0}", Math.floor(deltaSec / 3600)));
                    }
                } catch (ex) { alert(ex); }
            }
        }
        if (jsUtil.hasData("currentPlantPowerPointerAngle", "PlantPowerWidgetControl")) {
            adjustGauge(parseInt(jsUtil.getData("currentPlantPowerPointerAngle", "PlantPowerWidgetControl")));
        }
        if (typeof LiveProxyUtilities != "undefined") {
            var UpdateValue = function () {
                LiveProxyUtilities.GetLiveMeasurements("CurrentPlantPowerWidget", cbSuccess, cbError);
            }
            setInterval(UpdateValue, 5000);
        }
    });
    function adjustGauge(angle) {
        $("#CurrentPlantPowerPointer").rotate({
            duration: 1500,
            animateTo: angle
        });
    }
</script>
