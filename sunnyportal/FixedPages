<script type="text/javascript">
    var __lastStatusSuccess = null;
    function PlantStatusDone() {
        if ($("#plantStatusLoading").css("visibility") == "visible") {
            $("#plantStatusLoading").hide();
            $("#plantStatusDone").css("visibility", "visible");
        }
    }
    $(document).ready(function () {
        var cbSuccess = function (json, textStatus, jqXHR) {
            if (json != null) {
                __lastStatusSuccess = new Date().getTime();
                $("#CurrentPlantStatusTimestamp").html("&nbsp;");
                PlantStatusDone();
                if (json.OperationHealth != null) {
                    var linkText = "";
                    var imgUrl = "";
                    if (json.OperationHealth.Error == 1) {
                        linkText = jsUtil.getData("one_device_error", "PlantStatusWidgetControl");
                        imgUrl = "/Images/Dashboard/icon_AL_error.png";
                    } else if (json.OperationHealth.Error > 1) {
                        linkText = jsUtil.getData("X_devices_error", "PlantStatusWidgetControl").replace("{0}", json.OperationHealth.Error);
                        imgUrl = "/Images/Dashboard/icon_AL_error.png";
                    } else if (json.OperationHealth.Warning == 1) {
                        linkText = jsUtil.getData("one_device_warning", "PlantStatusWidgetControl");
                        imgUrl = "/Images/Dashboard/icon_AL_warning.png";
                    } else if (json.OperationHealth.Warning > 1) {
                        linkText = jsUtil.getData("X_devices_warning", "PlantStatusWidgetControl").replace("{0}", json.OperationHealth.Warning);
                        imgUrl = "/Images/Dashboard/icon_AL_warning.png";
                    } else if (json.OperationHealth.Ok > 0) {
                        linkText = jsUtil.getData("PlantLog_Title", "PlantStatusWidgetControl");
                        imgUrl = "/Images/Dashboard/icon_AL_ok.png";
                        if (json.OperationHealth.Unknown == 1) {
                            linkText = jsUtil.getData("one_device_unavailable", "PlantStatusWidgetControl");
                        } else if (json.OperationHealth.Unknown > 1) {
                            linkText = jsUtil.getData("X_devices_unavailable", "PlantStatusWidgetControl").replace("{0}", json.OperationHealth.Unknown);
                        }
                    } else {
                        linkText = jsUtil.getData("PlantLog_Title", "PlantStatusWidgetControl");
                        imgUrl = "/Images/Dashboard/icon_AL_waiting.png";
                    }
                    $("#LblPlantStatusLink").html(linkText);
                    $("#plantStatusIcon").attr("src", imgUrl);
                }
            }
        }
        var cbError = function (jqXHR, errorType, exception) {
            var elem = $("#CurrentPlantStatusTimestamp");
            if (elem != null && __lastStatusSuccess != null) {
                try {
                    var deltaSec = (new Date().getTime() - __lastStatusSuccess) / 1000;
                    if (deltaSec <= 15) {
                        elem.html("&nbsp;");
                    } else if (deltaSec <= 60) {
                        elem.html(jsUtil.getData("some_seconds_ago", "PlantPowerWidgetControl"));
                    } else if (deltaSec <= 900) {
                        elem.html(jsUtil.getData("just_now", "PlantPowerWidgetControl"));
                    } else if (deltaSec < 3600) {
                        elem.html(jsUtil.getData("x_minutes_ago", "PlantPowerWidgetControl").replace("{0}", Math.floor(deltaSec / 60)));
                    } else if (deltaSec < 7200) {
                        elem.html(jsUtil.getData("one_hours_ago", "PlantPowerWidgetControl"));
                    } else {
                        elem.html(jsUtil.getData("x_hours_ago", "PlantPowerWidgetControl").replace("{0}", Math.floor(deltaSec / 3600)));
                    }
                } catch (ex) { alert(ex); }
            }
        }
        if (typeof LiveProxyUtilities != "undefined") {
            var UpdateValue = function () {
                LiveProxyUtilities.GetLiveMeasurements("CurrentPlantStatusWidget", cbSuccess, cbError);
            }
            setInterval(UpdateValue, 5000);
            setTimeout(PlantStatusDone, 15000);
        }
    });
</script>
