    function CheckServiceAccess(ServiceID, FreeID, MailID) {
        if (gebi(ServiceID) && gebi(FreeID) && gebi(MailID)) {
            if (gebi(ServiceID).checked) {
                gebi(FreeID).checked = false;
                gebi(FreeID).disabled = true;
                gebi(MailID).style.display = "none";
            }
            else if (gebi(FreeID).checked) {
                gebi(ServiceID).checked = false;
                gebi(ServiceID).disabled = true;
                gebi(MailID).style.display = "";
            }
            else {
                gebi(FreeID).disabled = false;
                gebi(ServiceID).disabled = false;
                gebi(MailID).style.display = "none";
            }
        }
    }

    function CheckBoxFree(FreeID, MailID) {
        if (gebi(FreeID) && gebi(MailID)) {
            if (gebi(FreeID).checked) {
                gebi(MailID).style.display = "";
            }
            else {
                gebi(MailID).style.display = "none";
            }
        }
    }

    function ShowSendEMail() {
        $("#zoomDiagramCont").prop("onclick", null);
        $("#zoomDiagramCont").css("cursor", "auto");
        $("#zoomDiagramCont").css("backgroundColor", "transparent");
        $("#zoomDiagramCont").css("overflow", "hidden");
        $("#zoomDiagramCont").html("<div class=\"tabcontent\">" + $("#DivSendEMail").html() + "</div>");
        ZoomDiagramShow();
    }

    var sendMailID = "";

    function CheckMailButton() {
        if (gebi(sendMailID + "_SendMailButton")) {
            var disabledButton = false;
            if (!CheckMailFormat(gebi(sendMailID + "_EMailRecipientTextBox").value, ",")) {
                disabledButton = true;
                gebi("ImgEMailRecipient").src = "../Images/status_error_16.png";
            }
            else {
                gebi("ImgEMailRecipient").src = "../Images/status_Ok_16.png";
            }
            if (gebi(sendMailID + "_NameSenderTextBox").value.length <= 1) {
                disabledButton = true;
                gebi("ImgNameSender").src = "../Images/status_error_16.png";
            }
            else {
                gebi("ImgNameSender").src = "../Images/status_Ok_16.png";
            }
            if (disabledButton) {
                gebi(sendMailID + "_SendMailButton").style.display = "none";
                gebi(sendMailID + "_SendMailButtonDisabled").style.display = "";
            }
            else {
                gebi(sendMailID + "_SendMailButton").style.display = "";
                gebi(sendMailID + "_SendMailButtonDisabled").style.display = "none";
            }
        }
    }

    function SetSendMailID(id) {
        sendMailID = id;
    }

    function SetMailValues2Hidden() {
        if (gebi(sendMailID + "_EMailRecipientHidden")) {
            gebi(sendMailID + "_EMailRecipientHidden").value = gebi(sendMailID + "_EMailRecipientTextBox").value;
            gebi(sendMailID + "_EMailSenderHidden").value = gebi(sendMailID + "_EMailSenderTextBox").value;
            gebi(sendMailID + "_NameSenderHidden").value = gebi(sendMailID + "_NameSenderTextBox").value;
            gebi(sendMailID + "_MessageHidden").value = gebi(sendMailID + "_MessageTextBox").value;
            return true;
        }
        return false;
    }

var TabElementHeightArray = [];
var TabElementElement = null;
var TabElementID = null;

function switchTabElement(e, id) {
    TabElementElement = e;
    TabElementID = id;
    if (e.className.match(/closed/)) {
        e.className = e.className.replace(/closed/, "opened");
        if (e.parentNode)
            e.parentNode.className = e.parentNode.className.replace("tabheadclosed", "tabhead");
        if (e.parentNode.parentNode)
            e.parentNode.parentNode.className = e.parentNode.parentNode.className.replace("tabheadclosed", "tabhead");
        if (gebi(id + "Hid"))
            gebi(id + "Hid").value = "1";
        if (gebi(id + "Content1"))
            gebi(id + "Content1").className = "tabelle";
        if (gebi(id + "Content2")) {
            //slideTabElement(id + "Content2", 1);
            if (globalAnimateEffects) {
                $("#" + id + "Content2").slideDown("normal", function() {
                    endFktTabElement(id + "Content2", 1);
                });
            }
            else {
                gebi(id + "Content2").style.display = "block";
                endFktTabElement(id + "Content2", 1);
            }
        }
    }
    else {
        if (gebi(id + "Hid"))
            gebi(id + "Hid").value = "0";
        if (gebi(id + "Save"))
            gebi(id + "Save").style.display = "none";
        var slideStatus = -1;
        if (typeof(TabElementHeightArray[id + "Content2"]) == "undefined") { // wenn die Höhe noch nicht gesetzt ist, dann sofort schließen
            TabElementHeightArray[id + "Content2"] = gebi(id + "Content2").offsetHeight;
            slideStatus = 0;
            gebi(id + "Content2").style.display = "none";
        }
        if (gebi(id + "Content2")) {
            //slideTabElement(id + "Content2", slideStatus);
            if (globalAnimateEffects) {
                $("#" + id + "Content2").slideUp("normal", function() {
                    endFktTabElement(id + "Content2", -1);
                });
            }
            else {
                gebi(id + "Content2").style.display = "none";
                endFktTabElement(id + "Content2", -1);
            }
        }
    }
}

function endFktTabElement(divID, status) {
    if (status == 1) {
        if (gebi(TabElementID + "Save"))
            gebi(TabElementID + "Save").style.display = "block";
    }
    else {
        if (gebi(TabElementID + "Content1"))
            gebi(TabElementID + "Content1").className = "";
        if (TabElementElement.parentNode.parentNode)
            TabElementElement.parentNode.parentNode.className = TabElementElement.parentNode.parentNode.className.replace("tabhead", "tabheadclosed");
        if (TabElementElement.parentNode)
            TabElementElement.parentNode.className = TabElementElement.parentNode.className.replace("tabhead", "tabheadclosed");
        TabElementElement.className = TabElementElement.className.replace(/opened/, "closed");
    }
}

function slideTabElement(divID, status) {
    var slideStepHeight = 10;
    var slideIntervallTime = 15;
    var curDiv = gebi(divID);
    var newHeight = 0;
    var orgHeight = TabElementHeightArray[divID]; // Originalhöhe aus Array
    if (curDiv.style.height.length > 0)
        newHeight = parseInt(curDiv.style.height.substring(0, curDiv.style.height.indexOf('px'))); //Höhe ohne "px"
    else
        newHeight = orgHeight;
    if (isNaN(newHeight))
        return;

    newHeight = newHeight + (status * slideStepHeight);
    if (status == 0) // Sofort schließen
        newHeight = 0;

    if (newHeight > 0)
    {
        curDiv.style.display = "block";
        if (newHeight >= orgHeight) {
            curDiv.style.height = orgHeight.toString() + "px"; // Höhe auf Maximum setzen
            endFktTabElement(divID, status);
            return;
        }
    }
    else {
        curDiv.style.height = "0px"; // Höhe auf 0 setzen
        curDiv.style.display = "none";
        endFktTabElement(divID, status);
        return;
    }
    curDiv.style.height = newHeight.toString() + "px"; // Step-Höhe setzen
    setTimeout('slideTabElement("' + divID + '",' + status + ')', slideIntervallTime);
}

function setStatusDesignStyle(id) {
    $("#" + id).find("td").css("vertical-align", "top");
    $("#" + id).find("img").css("margin-bottom", "-25px");
}

-->
</script>

        </div>
   </div>

                                        <div id="MouseTitleDiv" style="position:absolute; top:0px; left:0px;z-index:10000;"></div>
                                    </div>
                                </td>

                            </tr>
                        </table>

